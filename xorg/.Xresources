urxvt*depth: 32

! disable perl extensions cause slow
urxvt*perl-ext:
urxvt*perl-ext-common:

! fonts
urxvt*font: -kakwa-kakwafont-medium-r-normal--12-120-72-72-c-60-iso8859-1
urxvt*boldFont: -kakwa-kakwafont-medium-r-bold--12-120-72-72-c-60-iso8859-1
urxvt*italicFont:

urxvt*letterSpace: 0
urxvt*lineSpace: 0

urxvt*scrollBar:
urxvt*pointerBlank: true

! colors
urxvt*foreground: #FFFFFF
urxvt*background: [80]#000000

!black
*color8:           #656565
*color0:           #565656
!red
*color1:           #AC4F4F
*color9:           #E85555
!green
*color2:           #88AB77
*color10:          #8CCD6C
!yellow
*color3:           #D0D9AD
*color11:          #C5D67D
!blue
*color4:           #677088
*color12:          #647396
!magenta
*color5:           #CDCDB4
*color13:          #CDB4C9
!cyan
*color6:           #445362
*color14:          #505B6C
!white
*color7:           #FFFFFF
*color15:          #C0C0C0
